<footer>
  <p class="footer-left">
  <% if (config.author){ %>
  &copy; <%= new Date().getFullYear() %> <%= config.author %>
  <% } else { %>
  &copy; <%= new Date().getFullYear() %> <%= config.title %>
  <% } %>
  <span>|</span>
  Powered by: <a href="https://github.com/tommy351/hexo">Hexo</a> <span>|</span>
  Designed by: <a href="http://styleshout.com/">styleshout</a>
  </p>
  <p class="footer-right">
    <% if (theme.twitter.username) { %>
      <a href="https://twitter.com/<%= theme.twitter.username %>" class="fa fa-twitter">&nbsp;</a>
     <% } %>
    <% if (theme.facebook.username) { %>
      <a href="https://facebook.com/<%= theme.facebook.username %>" class="fa fa-facebook">&nbsp;</a>
     <% } %>
    <% if (theme.google_plus.username) { %>
      <a href="https://plus.google.com/+<%= theme.google_plus.username %>" class="fa fa-google-plus">&nbsp;</a>
     <% } %>
  </p>
</footer>
